<Model>SML</Model>

<Sensors>
    <Sensor id="1">
	<Name>Nivel deposito Rochester Junior</Name>
	<Measure>
		<PhenomenonApp></PhenomenonApp>
		<PhenomenonIdas>fillLevel</PhenomenonIdas>
		<Uom>percent</Uom>
		<ParamRef>presence</ParamRef>
		<Param>Rochester Junior</Param>
	</Measure>
	<Alarm id="0">		
		<Phenomenon>minLevelAlarm</Phenomenon>
	</Alarm>
	<Alarm id="1">		
		<Phenomenon>medLevelAlarm</Phenomenon>
	</Alarm>
	<Alarm id="2">		
		<Phenomenon>maxLevelAlarm</Phenomenon>		
	</Alarm>
    </Sensor>
    <Sensor id="2">
	<Name>Nivel deposito Rochester Senior</Name>
	<Measure>
		<PhenomenonApp></PhenomenonApp>
		<PhenomenonIdas>fillLevel</PhenomenonIdas>
		<Uom>percent</Uom>
		<ParamRef>presence</ParamRef>
		<Param>Rochester Senior</Param>
	</Measure>
	<Alarm id="0">		
		<Phenomenon>minLevelAlarm</Phenomenon>
	</Alarm>
	<Alarm id="1">		
		<Phenomenon>medLevelAlarm</Phenomenon>
	</Alarm>
	<Alarm id="2">		
		<Phenomenon>maxLevelAlarm</Phenomenon>		
	</Alarm>
    </Sensor>      
    <Sensor id="3">
        <Name>Nivel deposito Magnetel</Name>
	<Measure>
		<PhenomenonApp></PhenomenonApp>
		<PhenomenonIdas>fillLevel</PhenomenonIdas>
		<Uom>percent</Uom>
		<ParamRef>presence</ParamRef>
		<Param>Magnetel</Param>
	</Measure>
	<Alarm id="0">		
		<Phenomenon>minLevelAlarm</Phenomenon>
	</Alarm>
	<Alarm id="1">		
		<Phenomenon>medLevelAlarm</Phenomenon>
	</Alarm>
	<Alarm id="2">		
		<Phenomenon>maxLevelAlarm</Phenomenon>		
	</Alarm>
   </Sensor>
   <Sensor id="4">
        <Name>Presion 2,5 bar</Name>
	<Measure>
		<PhenomenonApp></PhenomenonApp>
		<PhenomenonIdas>pressure</PhenomenonIdas>
		<Uom>bar</Uom>
		<ParamRef>presence</ParamRef>
		<Param>Presion 2,5</Param>
		<Transform>0.009</Transform>
	</Measure>
	<Alarm id="0">		
		<Phenomenon>minPressureAlarm</Phenomenon>
	</Alarm>
	<Alarm id="1">		
		<Phenomenon>medPressureAlarm</Phenomenon>
	</Alarm>
	<Alarm id="2">		
		<Phenomenon>maxPressureAlarm</Phenomenon>		
	</Alarm>
   </Sensor>
   <Sensor id="5">
        <Name>Presion 16 bar</Name>
	<Measure>
		<PhenomenonApp></PhenomenonApp>
		<PhenomenonIdas>pressure</PhenomenonIdas>
		<Uom>bar</Uom>
		<ParamRef>presence</ParamRef>
		<Param>Presion 16</Param>
		<Transform>0.06</Transform>	
	</Measure>
	<Alarm id="0">		
		<Phenomenon>minPressureAlarm</Phenomenon>
	</Alarm>
	<Alarm id="1">		
		<Phenomenon>medPressureAlarm</Phenomenon>
	</Alarm>
	<Alarm id="2">		
		<Phenomenon>maxPressureAlarm</Phenomenon>		
	</Alarm>
   </Sensor>
   <Sensor id="6">
        <Name>Tension catodica</Name>
	<Measure>
		<PhenomenonApp></PhenomenonApp>
		<PhenomenonIdas>electricPotential</PhenomenonIdas>
		<Uom>volt</Uom>
		<Transform>-0.005</Transform>
	</Measure>
	<Alarm id="0">		
		<Phenomenon>minPotentialAlarm</Phenomenon>
	</Alarm>
	<Alarm id="1">		
		<Phenomenon>medPotentialAlarm</Phenomenon>
	</Alarm>
	<Alarm id="2">		
		<Phenomenon>maxPotentialAlarm</Phenomenon>		
	</Alarm>
   </Sensor>
   <Sensor id="7">
        <Name>Tension de bateria</Name>
	<Measure>
		<PhenomenonApp></PhenomenonApp>
		<PhenomenonIdas>batteryCharge</PhenomenonIdas>
		<Uom>volt</Uom>
		<Transform>0.078</Transform>
	</Measure>
	<Alarm id="0">		
		<Phenomenon>minBattery</Phenomenon>
	</Alarm>
	<Alarm id="1">		
		<Phenomenon>medBattery</Phenomenon>
	</Alarm>
	<Alarm id="2">		
		<Phenomenon>maxBattery</Phenomenon>		
	</Alarm>
   </Sensor>
   <Sensor id="signalquality">
	<Name>Calidad Senal GSM</Name>
	<Measure>
		<PhenomenonApp></PhenomenonApp>
		<PhenomenonIdas>signalQuality</PhenomenonIdas>
		<Uom>receivedSignalStrengthIndicator</Uom>
	</Measure>
  </Sensor>
</Sensors>

<SMPP>
    <HOST><%=@smpp_adapter_host%></HOST>
    <PORT><%=@smpp_adapter_port%></PORT>
    <SEND>
	<URL><%=@smpp_adapter_url%></URL>
	<METHOD>POST</METHOD>
	<FROM><%=@from_number%></FROM>
	<TRIES>1</TRIES>
    </SEND>
</SMPP>

<NetrisSensors>
  <Sensor id="input1">
	<Name>Nivel deposito</Name>
	<Measure>
		<PhenomenonApp></PhenomenonApp>
		<PhenomenonIdas>fillLevel</PhenomenonIdas>
		<Uom>percent</Uom>
	</Measure>
  </Sensor>  
  <Sensor id="temperature">
	<Name>Temperatura deposito</Name>
	<Measure>
		<PhenomenonApp></PhenomenonApp>
		<PhenomenonIdas>temperature</PhenomenonIdas>
		<Uom>celsius</Uom>
	</Measure>
  </Sensor>
  <Sensor id="batterylevel">
	<Name>Carga Bateria</Name>
	<Measure>
		<PhenomenonApp></PhenomenonApp>
		<PhenomenonIdas>batteryCharge</PhenomenonIdas>
		<Uom>volt</Uom>
	</Measure>
  </Sensor>
  <Sensor id="signalquality">
	<Name>Calidad Senal GSM</Name>
	<Measure>
		<PhenomenonApp></PhenomenonApp>
		<PhenomenonIdas>signalQuality</PhenomenonIdas>
		<Uom>receivedSignalStrengthIndicator</Uom>
	</Measure>
  </Sensor>
</NetrisSensors>
